{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "catanie": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.json",
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": ["node_modules"]
            },
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/app/loopback_form",
              "src/android-chrome-192x192.png",
              "src/apple-touch-icon.png",
              "src/favicon-16x16.png",
              "src/favicon-32x32.png",
              "src/mstile-150x150.png",
              "src/safari-pinned-tab.svg"
            ],
            "styles": ["src/styles.css", "src/theme.scss"],
            "scripts": ["node_modules/hammerjs/hammer.min.js"]
          },
          "configurations": {
            "dmscdev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dmscdev.ts"
                },
                {
                  "replace": "src/assets/images/ess-site.png",
                  "with": "CI/ESS/ess-site.png"
                }
              ]
            },
            "dmsc": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dmsc.ts"
                }
              ]
            },
            "ess": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.ess.ts"
                }
              ]
            },
            "dmscprod": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dmscprod.ts"
                }
              ]
            },
            "maxiv-prod": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.maxiv.prod.ts"
                }
              ]
            },
            "development": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                },
                {
                  "replace": "src/assets/images/ess-site.png",
                  "with": "CI/PSI/site.png"
                }
              ]
            },
            "dmscproduction": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/assets/images/ess-site.png",
                  "with": "CI/PSI/site.png"
                },
                {
                  "replace": "src/assets/images/esslogo.png",
                  "with": "CI/PSI/site-logo.png"
                }
              ]
            },
            "staging": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                },
                {
                  "replace": "src/assets/images/ess-site.png",
                  "with": "CI/PSI/site.png"
                },
                {
                  "replace": "src/assets/images/esslogo.png",
                  "with": "CI/PSI/site-logo.png"
                }
              ]
            },
            "qa": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.qa.ts"
                }
              ]
            },
            "docker": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.docker.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "catanie:build",
            "port": 4200,
            "host": "0.0.0.0"
          },
          "configurations": {
            "dmscdev": {
              "browserTarget": "catanie:build:dmscdev"
            },
            "dmsc": {
              "browserTarget": "catanie:build:dmsc"
            },
            "ess": {
              "browserTarget": "catanie:build:ess"
            },
            "dmscprod": {
              "browserTarget": "catanie:build:dmscprod"
            },
            "maxiv-prod": {
              "browserTarget": "catanie:build:maxiv-prod"
            },
            "development": {
              "browserTarget": "catanie:build:development"
            },
            "dmscproduction": {
              "browserTarget": "catanie:build:dmscproduction"
            },
            "production": {
              "browserTarget": "catanie:build:production"
            },
            "staging": {
              "browserTarget": "catanie:build:staging"
            },
            "qa": {
              "browserTarget": "catanie:build:qa"
            },
            "docker": {
              "browserTarget": "catanie:build:docker"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "catanie:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "src/tsconfig.json",
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "scripts": ["node_modules/hammerjs/hammer.min.js"],
            "styles": ["src/styles.css", "src/theme.scss"],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/app/loopback_form",
              "src/android-chrome-192x192.png",
              "src/apple-touch-icon.png",
              "src/favicon-16x16.png",
              "src/favicon-32x32.png",
              "src/mstile-150x150.png",
              "src/safari-pinned-tab.svg"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.json"],
            "exclude": []
          }
        }
      }
    },
    "catanie-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "catanie:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "catanie",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
