{
  "type": "VerticalLayout",
  "elements": [
    {
      "type":"Group", 
      "elements":[
        {
          "type": "Control",
          "label": "Title",
          "scope": "#/properties/title",
          "options":{
            "multi":true
          }
        },
        {
          "type": "Control",
          "label": "Release Date",
          "scope": "#/properties/release_date",
          "options": {
            "format": "radio"
          }
        },
        {
          "type": "Control",
          "label": "Experiment Type",
          "scope": "#/properties/experiment_type",
          "options": {
            "format": "radio"
          }
        },
        {
          "type": "Control",
          "label":"Scale",
          "scope": "#/properties/scale",
          "options": {
            "format": "select"
          }
        }, 
        {
          "type": "Group",
          "elements": [
            {
              "type": "Label",
              "text": "EMDB References"
            },
            {
              "type": "Control",
              "scope": "#/properties/cross_references",
              "options": {
                "elementLabelProp": "name"
              }
            },
            {
              "type": "Label",
              "text": "BioStudies References"
            },
            {
              "type": "Control",
              "scope": "#/properties/biostudies_references",
              "options": {
                "elementLabelProp": "name"
              }
            },
            {
              "type": "Label",
              "text": "IDR References"
            },
            {
              "type": "Control",
              "scope": "#/properties/idr_references",
              "options": {
                "elementLabelProp": "name"
              }
            },
            {
              "type": "Label",
              "text": "EMPIAR References"
            },
            {
              "type": "Control",
              "scope": "#/properties/empiar_references",
              "options": {
                "elementLabelProp": "name"
              }
            },
            {
              "type": "Label",
              "text": "Workflows"
            },
            {
              "type": "Control",
              "label": "Workflows",
              "scope": "#/properties/workflows",
              "options": {
                "elementLabelProp": "url"
              }
            }
          ]
        }
      ]
    }
  ]
}